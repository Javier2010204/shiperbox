<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Birthday</th>
      <th>Nit</th>
      <th>Tax residence</th>
      <th>Business name</th>
      <th>Address</th>
      <th>Delivery instructions</th>
      <th>Contact name1</th>
      <th>Contact phone1</th>
      <th>Contact name2</th>
      <th>Contact phone2</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.name %></td>
        <td><%= profile.phone %></td>
        <td><%= profile.birthday %></td>
        <td><%= profile.nit %></td>
        <td><%= profile.tax_residence %></td>
        <td><%= profile.business_name %></td>
        <td><%= profile.address %></td>
        <td><%= profile.delivery_instructions %></td>
        <td><%= profile.contact_name1 %></td>
        <td><%= profile.contact_phone1 %></td>
        <td><%= profile.contact_name2 %></td>
        <td><%= profile.contact_phone2 %></td>
        <td><%= profile.user_id %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
