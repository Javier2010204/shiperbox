<p id="notice"><%= notice %></p>

<h1>Quotes</h1>

<table>
  <thead>
    <tr>
      <th>Product price</th>
      <th>Shipping us</th>
      <th>Product weight</th>
      <th>Category</th>
      <th>Shiperbox buy</th>
      <th>Home service</th>
      <th>Home service option</th>
      <th>Link</th>
      <th>Total</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotes.each do |quote| %>
      <tr>
        <td><%= quote.product_price %></td>
        <td><%= quote.shipping_us %></td>
        <td><%= quote.product_weight %></td>
        <td><%= quote.category %></td>
        <td><%= quote.shiperbox_buy %></td>
        <td><%= quote.home_service %></td>
        <td><%= quote.home_service_option %></td>
        <td><%= quote.link %></td>
        <td><%= quote.total %></td>
        <td><%= link_to 'Show', quote %></td>
        <td><%= link_to 'Edit', edit_quote_path(quote) %></td>
        <td><%= link_to 'Destroy', quote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quote', new_quote_path %>
